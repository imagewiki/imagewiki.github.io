                .radio
                  label
                    input#reason_Spam(name='reason', type='radio', value='Spam')
                    | Spam
                    span.badge(data-placement='right', data-toggle='tooltip', title='', data-original-title='This page contains misleading or massively posted content.')
                      i.fa.fa-question
                br
                label(for='description') Comments
                br
                textarea#description.form-control(name='description')
                br
                br
                input.send-form-violations.btn.btn-primary(name='commit', style='display: none;', type='submit', value='Send')
              .errors.alert.alert-danger
                strong Reason:
                ul
              .violations-feedback
                p.feedback-msg Report sent!
